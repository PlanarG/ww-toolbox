{
  "name": "ww",
  "version": "0.2.0",
  "description": "Wuthering Waves Toolbox",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "electron": "^36.4.0"
  },
  "dependencies": {
    "@mdi/font": "^7.4.47",
    "is-admin": "^3.0.0",
    "tailwindcss": "^3.4.1",
    "tree-kill": "^1.2.2"
  },
  "build": {
    "appId": "com.wutheringwaves.toolbox",
    "files": [
      "main.js",
      "index.html",
      "page1.js",
      "page1.html",
      "page2.html",
      "page3.html",
      "styles.css"
    ],
    "extraResources": [
      {
        "from": "preload.js",
        "to": "preload.js"
      },
      {
        "from": "backend",
        "to": "backend"
      },
      {
        "from": "../assets",
        "to": "assets",
        "filter": [
          "!**/*.py"
        ]
      }
    ],
    "productName": "Wuthering Waves Toolbox",
    "copyright": "Copyright Â© 2025 PlanarG",
    "win": {
      "icon": "../assets/icon.ico",
      "target": ["nsis"]
    }
  }
}
